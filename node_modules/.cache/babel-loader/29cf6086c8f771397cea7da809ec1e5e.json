аза\n                <span>\n                    {{total}} тг\n                </span>\n            </p>\n        </div>\n    </div>\n</div>\n</template>\n\n<script>\nexport default {\n  name: 'MyOrder',\n  props: ['data', 'cut', 'wrap'],\n  data() {\n        return {\n            price: 120,\n            select: this.data.selected,\n            total: 0,\n        }\n  },\n  created() {\n      this.total = this.newTotal\n  },\n  computed:{\n      newTotal(){\n          let t = 0;\n          this.select.forEach(s => {\n              t += this.price * s.weight;\n          });\n          return t\n      }\n  },\n  methods: {\n        calculatePrice(weight){\n            return weight * this.price;\n        },\n\n        deleteOrder(el) {\n            el.status = \"Ripe\";\n            this.select = this.select.filter(t => t !== el);\n\n\n            this.$emit('selected', {\n                selected: this.select,\n            });\n        },\n        deleteCut(){\n            this.$emit('cut_wrap', {\n                cut: false,\n                wrap: this.wrap\n            });\n        },\n        deleteWrap(){\n            this.$emit('cut_wrap', {\n                cut: this.cut,\n                wrap: false\n            });\n        },\n  },\n  watch: {\n      select(){\n        this.total = this.newTotal\n      },\n      data(){\n        this.select = this.data.selected\n      },\n      cut() {\n          if(this.cut){\n              this.total += 200;\n          }\n          else if (!this.cut){\n              this.total -= 200;\n          }\n      },\n      wrap() {\n          if(this.wrap){\n              this.total += 500;\n          }\n          else if (!this.wrap){\n              this.total -= 500;\n          }\n      }\n  }\n}\n</script>\n\n<style scoped>\n.myOrder {\n    margin-top: 50px;\n    text-align: left;\n    padding: 40px;\n    border-radius: 10px;\n    -webkit-box-shadow: 8px 7px 30px -10px rgba(30, 39, 35, 0.31);\n    -moz-box-shadow: 8px 7px 30px -10px rgba(30, 39, 35, 0.31);\n    box-shadow: 8px 7px 30px -10px rgba(30, 39, 35, 0.31);\n\n    font-size: 20px;\n}\nh2 {\n    margin: 0;\n    color: #309A00;\n    margin-bottom: 20px;\n}\n.order_list {\n    display: flex;\n    justify-content: space-between;\n    align-items:flex-start;\n    padding: 15px 0;\n    border-bottom: 0.05rem solid rgba(213, 213, 213, 0.6);\n}\n.list_info span {\n    color: #309A00;\n    font-weight: 700;\n}\ndiv .list_info {\n    margin: 0;\n}\n.price {\n    margin: 0;\n    margin-top: 5px;\n}\n.price span{\n    border-radius: 5px;\n    padding: 5px;\n    font-weight: 700;\n    background-color: #309A00;\n    color: #fff;\n}\n\nsvg{\n    color: #FF2600;\n    width: 40px;\n    cursor: pointer;\n}\n\nsvg:hover {\n    color: #921600;\n}\n\n.services {\n    margin-top: 20px;\n    display: flex;\n    justify-content: space-between;\n}\n\n.cut .price {\n    margin-left: 19px;\n}\n\n.total{\n    margin-top: 50px;\n    display: flex;\n    justify-content: end;\n}\n\n.total .price{\n    font-size: ;\n}\n</style>\n"]}]}